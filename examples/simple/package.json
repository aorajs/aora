{
  "name": "example-simple",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aora/react": "workspace:*",
    "@aora/server": "workspace:*",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "aora": "workspace:*",
    "cross-env": "^7.0.0",
    "pm2": "^4.5.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.10",
    "serialize-javascript": "^6.0.0",
    "swiper": "6.7.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.5",
    "@aora/cli": "workspace:*",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-standard-react-ts": "^1.0.5",
    "path-to-regexp": "^6.2.0",
    "typescript": "^4.0.0",
    "webpack": "^4.46.0"
  },
  "scripts": {
    "prod": "ssr build && pm2 start pm2.config.js",
    "prod:vite": "ssr build --vite && cross-env BUILD_TOOL=vite pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "start": "aora dev",
    "serve": "aora serve",
    "start:vite": "ssr start --vite",
    "routes": "DEBUG=ssr:parse aora routes",
    "build": "aora build",
    "build:vite": "ssr build --vite",
    "deploy": "ssr build && ssr deploy",
    "deploy:tencent": "ssr build && ssr deploy --tencent",
    "lint": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:fix": "eslint . --ext .js,.tsx,.ts --cache --fix"
  }
}
