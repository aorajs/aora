{
  "name": "example-simple",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "aora": "^0.2.35",
    "cross-env": "^7.0.0",
    "pm2": "^4.5.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "serialize-javascript": "^6.0.0",
    "swiper": "6.7.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.3",
